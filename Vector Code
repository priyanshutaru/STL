#include <bits/stdc++.h>
#include <iostream>
using namespace std;
 void printvec(vector<string> v){
   cout<<"size of  vector"<<" "<<v.size()<<endl;
   for(int i=0;i<v.size();i++){
     cout<<v[i]<<" ";
   }
  cout<<endl;
}


int main() {
    vector<string>v;
    int n;
    cin>>n;
    for(int i=0;i<n;++i){
        string x;
        cin>>x;
        v.push_back(x);
    }
    printvec(v)
    
    
    
    
    
    
